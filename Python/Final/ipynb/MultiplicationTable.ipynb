   "source": [
    "try:\n",
    "    number = int(input(\"Enter a number: \"))\n",
    "    for i in range(1, 11):\n",
    "        print(f\"{number} x {i} = {number * i}\")\n",
    "except Exception as e:\n",
    "    print(f\"Error: {e}\")"
